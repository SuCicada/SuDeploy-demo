<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>com.peng.supervisord</string>

        <key>Disabled</key>
        <false/>

        <key>KeepAlive</key>
        <true/>

        <key>ProcessType</key>
        <string>Background</string>

        <key>ProgramArguments</key>
        <array>
            <string>/opt/homebrew/anaconda3/bin/supervisord</string>
            <string>-c</string>
            <string>/Users/peng/.config/supervisor/supervisord.ini</string>
        </array>

        <key>RunAtLoad</key>
        <true/>

        <key>StandardErrorPath</key>
        <string>/tmp/local.job.err</string>
        <key>StandardOutPath</key>
        <string>/tmp/local.job.out</string>

        <!--        <key>UserName</key>-->
<!--        <string>root</string>-->

<!--        <key>GroupName</key>-->
<!--        <string>wheel</string>-->
    </dict>
</plist>
